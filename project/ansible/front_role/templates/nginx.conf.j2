# {{ ansible_managed }}
events {
    worker_connections 1024;
}

http {
    server {
        listen       80;
	    listen	443 ssl;
        server_name  {{ common_name }};
        root         /usr/share/nginx/html;
	    ssl_certificate {{ cert_path }};
	    ssl_certificate_key {{ private_key_path }};

        location / {
        }
    }
}

